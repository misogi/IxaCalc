<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<ControlTemplate x:Key="DeckedBushoListItemTemplate" TargetType="ListBoxItem">
		<StackPanel HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Width="20">
			<Image Height="100"/>
			<TextBlock TextWrapping="Wrap" Text="{Binding Name}"/>
		</StackPanel>
	</ControlTemplate>
	<!-- リソース ディクショナリのエントリはここで定義します。-->
</ResourceDictionary>